{
    "manifest_version": 3,
    "name": "Typewriter by Tahir Naseer",
    "version": "2.0",
    "description": "An advanced tool to paste content in different styles with customizable settings.",
    "permissions": ["clipboardRead", "clipboardWrite"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }